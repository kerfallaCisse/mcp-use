{
  "mcpServers": {
    "airbnb": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@openbnb/mcp-server-airbnb", "--ignore-robots-txt"]
    },
    "playwright": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@playwright/mcp@latest"],
      "env": {
        "DISPLAY": ":1"
      }
    },
    "notes": {
      "url": "http://localhost:5000/sse"
    }
  }
}